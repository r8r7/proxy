{
  "version": 2,
  "builds": [{
      "src": "index.js",
      "use": "@vercel/node"
  }],
  "routes": [{
      "src": "/(.*)",
      "dest": "index.js"
  }]
},
{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://r8r7.github.io"
        }
      ]
    }
  ]
}
